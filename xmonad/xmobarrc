Config {
    font = "xft:DejaVu Sans Mono for Powerline:pixelsize=13:antialias=true:hinting=true"
  , bgColor = "#000000"
  , fgColor = "#ffffff"
  , position = TopW L 100
  , commands = [
      Run Cpu ["-L","3","-H","50","--low","#6666ff","--high","#ff0000"] 10
    , Run Date "%a %d/%m-%y %H:%M" "date" 10
    , Run StdinReader
  ],
  , sepChar = "%"
  , alignSep = "}{"
  , template = "%StdinReader% }{ %cpu% | <fc=#66ff66>%date%</fc> "
}
